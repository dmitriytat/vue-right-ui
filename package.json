{
  "name": "vue-right-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "styleguide:run": "yarn run styleguide:build && http-server ./styleguide -p 6060",
    "test:coveralls": "yarn run test:unit && cat ./coverage/lcov.info | coveralls",
    "test:e2e": "wait-on http://localhost:6060 && cypress run",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-bem-cn": "^1.1.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-e2e-cypress": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.2.2",
    "http-server": "^0.11.1",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^8.1.5",
    "postcss-nested": "^4.1.2",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "vue-cli-plugin-postcss-preset": "^1.0.2",
    "vue-cli-plugin-styleguidist": "^3.10.2",
    "vue-cli-plugin-svg-sprite": "^0.3.1",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "wait-on": "^3.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "import/prefer-default-export": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/src/**/*.spec.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{vue,htm,html,css,sss,less,scss}": [
      "vue-cli-service lint:style",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 0
      },
      "postcss-nested": {}
    }
  }
}

{
  "name": "vue-right-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "lint:scripts": "eslint --ext .js,.vue ./src",
    "lint:styles": "stylelint './src/**/*.{css,vue}' --config .stylelintrc",
    "test": "export NODE_ENV=test; jest",
    "lint-staged": "lint-staged",
    "lint:scripts:staged": "eslint",
    "lint:styles:staged": "stylelint --config .stylelintrc"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-bem-cn": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@vue/babel-preset-app": "^3.5.3",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "stylelint": "^9.10.1",
    "stylelint-config-rational-order": "0.0.4",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.6.10",
    "vue-styleguidist": "3.10.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6"
  },
  "lint-staged": {
    "src/**/*.vue": [
      "npm run lint:scripts:staged -- --fix",
      "npm run lint:styles:staged -- --fix",
      "git add"
    ],
    "src/**/*.js": [
      "npm run lint:scripts:staged -- --fix",
      "git add"
    ],
    "src/**/*.css": [
      "npm run lint:styles:staged -- --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run test"
    }
  }
}
